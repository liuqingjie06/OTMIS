<form class="form-signin" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    <div>
       {{ form_errors(form.username) }}
    	<label>名   字 </label>  
  		{{ form_widget(form.username) }}
    </div>
    <div>
         {{ form_errors(form.email) }}
        <label>邮     箱 </label>  
    	
    	{{ form_widget(form.email) }}
    </div>

    {{ form_rest(form)}}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}"  />
    </div>
</form>
